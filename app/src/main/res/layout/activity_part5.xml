<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:background="#ffc822"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".part5">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/quiz9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/quiz9"
            android:textSize="15sp"
            android:textStyle="bold"
            android:textAllCaps="false"
            android:textColor="#80033b"

            />
        <RadioGroup
            android:id="@+id/groupId9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <RadioButton
                android:id="@+id/op91"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/option91"
                android:textAllCaps="false"
                android:textStyle="bold"
                android:textSize="15sp"
                android:textColor="#80033b"

                />
            <RadioButton
                android:id="@+id/op92"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/option92"
                android:textAllCaps="false"
                android:textStyle="bold"
                android:textSize="15sp"
                android:textColor="#80033b"

                />

            <RadioButton
                android:id="@+id/op93"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/option93"
                android:textAllCaps="false"
                android:textStyle="bold"
                android:textSize="15sp"
                android:textColor="#80033b"

                />
            <RadioButton
                android:id="@+id/op94"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/option94"
                android:textAllCaps="false"
                android:textStyle="bold"
                android:textSize="15sp"
                android:textColor="#80033b"


                />


        </RadioGroup>



        <Button
            android:id="@+id/bu9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Submit"
            android:textSize="15sp"
            android:textColor="#1b1d1c"
            android:textAllCaps="false"
            />

        <TextView
            android:id="@+id/res9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textStyle="bold"
            android:textAllCaps="false"
            android:textColor="#7612e9"
            android:layout_marginBottom="5dp"

            />

        <TextView
            android:id="@+id/quiz10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/quiz10"
            android:textSize="15sp"
            android:textStyle="bold"
            android:textAllCaps="false"
            android:textColor="#80033b"

            />
        <RadioGroup
            android:id="@+id/groupId10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <RadioButton
                android:id="@+id/op101"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/option101"
                android:textAllCaps="false"
                android:textStyle="bold"
                android:textSize="15sp"
                android:textColor="#80033b"

                />
            <RadioButton
                android:id="@+id/op102"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/option102"
                android:textAllCaps="false"
                android:textStyle="bold"
                android:textSize="15sp"
                android:textColor="#80033b"

                />

            <RadioButton
                android:id="@+id/op103"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/option103"
                android:textAllCaps="false"
                android:textStyle="bold"
                android:textSize="15sp"
                android:textColor="#80033b"

                />
            <RadioButton
                android:id="@+id/op104"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/option104"
                android:textAllCaps="false"
                android:textStyle="bold"
                android:textSize="15sp"
                android:textColor="#80033b"


                />


        </RadioGroup>



        <Button
            android:id="@+id/bu10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Submit"
            android:textSize="15sp"
            android:textColor="#1b1d1c"
            android:textAllCaps="false"
            />

        <TextView
            android:id="@+id/res10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textStyle="bold"
            android:textAllCaps="false"
            android:textColor="#7612e9"
            android:layout_marginBottom="5dp"

            />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/back4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="100dp"
                android:text="Back"
                android:textAllCaps="false"
                android:textColor="#80033b"
                android:textSize="15sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/next4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="100dp"
                android:baselineAligned="false"
                android:text="Finish"
                android:textAllCaps="false"
                android:textColor="#80033b"
                android:textSize="15sp"
                android:textStyle="bold" />

        </LinearLayout>

    </LinearLayout>

</ScrollView>